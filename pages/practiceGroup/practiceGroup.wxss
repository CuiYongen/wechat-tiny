.icontainer {
  background: #FFFFFF;
  overflow: hidden;
  min-height: 100vh;
  box-sizing: border-box;
  color:#343B41;
}
.icontainer::before {
  position: fixed;
  top: 0;
  left: 0;
  content: ' ';
  width: 100%;
  height: 1rpx;
  background-color: #e2e2e2;
  z-index: 5;
}
.demo {
  padding: 0px 0;
  display: flex;
  flex-flow:column wrap;
  justify-content: center;
}
.demo__item {
  margin-left:15rpx;
  margin-right:15rpx;
}
.demo__icon {
  margin: 0 auto;
  width: 72px;
  height: 72px;
  background: #ddd;
  border-radius: 8px;
}
.doc-title {
  font-size: 23px;
  line-height: 25px;
  color: #343B41;
  padding: 30px 0;
  margin: 30px 15px 20px 15px;
  text-align:center;
}

.zan-badge {
  position: relative;
}
.zan-badge__count {
  position: absolute;
  top: -8px;
  right: 0px;
  height: 1.6em;
  min-width: 1.6em;
  line-height: 1.6;
  padding: 0 .4em;
  font-size: 10px;
  font-family: tahoma;
  border-radius: .8em;
  background: #FF4444;
  color: #343B41;
  text-align: center;
  white-space: nowrap;
  transform: translateX(50%);
  transform-origin: -10% center;
  z-index: 10;
  box-shadow: 0 0 0 1px #343B41;
  box-sizing: border-box;
}

.cynsytle{
	display:flex;
	background:transparent;
	flex-direction:column;
	align-items:center;
	justify-content:center;
	border:1px solid #000000;

}
.oneline{
	display: flex;
	margin-top:30rpx;
}
.alarm{
  display: flex;
  margin-top: 30rpx;
  color:#DDDDDD;
  justify-content: center;
  align-items: center;
  visibility: hidden;
}
.alarmTips{
  letter-spacing: 0.1em;
  padding-left: 16rpx;
}