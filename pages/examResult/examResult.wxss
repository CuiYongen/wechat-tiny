/* examResult.wxss */
.flex-wrp{
    display:flex;
    justify-content:center;
    background-color: #FFFFFF;
 }
 .flex-item{
    width: 100px;

}
.flex-item.head{
    text-align: center;
    margin-top: 80rpx;
    width:100%;
}
.flex-item.score{
    text-align: center;
    margin-top: 30rpx;
    font-size: 100rpx;
    color:#DDDDDD;
    width:100%;
}
.flex-item.comment{
    margin: 50rpx 100rpx 100rpx 100rpx;
    width: 80%;
    height: 400rpx;
    align-self:center;
    line-height:65rpx;
}
.flex-item.button{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    height: 100px;
}
.flex-item.examInfo{
    width: auto;
    justify-content: center;
    align-self:center;
    height: 100px;
    display: flex;
}